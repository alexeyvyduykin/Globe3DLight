<Application x:Class="Globe3DLight.App"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:theme="using:Globe3DLight.Themes"
             xmlns:local="clr-namespace:Globe3DLight">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        
        <theme:BlenderTheme></theme:BlenderTheme>

        <StyleInclude Source="avares://Globe3DLight/Icons.axaml"/>

        <StyleInclude Source="avares://Globe3DLight/Styles/EditorStyle.axaml"/>

    </Application.Styles>
</Application>
