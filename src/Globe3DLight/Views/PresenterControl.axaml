<UserControl x:Class="Globe3DLight.Views.PresenterControl"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:i="clr-namespace:Avalonia.Xaml.Interactivity;assembly=Avalonia.Xaml.Interactivity"
             xmlns:ia="clr-namespace:Avalonia.Xaml.Interactions.Core;assembly=Avalonia.Xaml.Interactions"
             xmlns:iac="clr-namespace:Avalonia.Xaml.Interactions.Custom;assembly=Avalonia.Xaml.Interactions.Custom"
             xmlns:Editor="clr-namespace:Globe3DLight.ViewModels.Editor;assembly=Globe3DLight"
            
             Focusable="True"  
             xmlns:Designer="clr-namespace:Globe3DLight.ViewModels.Designer;assembly=Globe3DLight"  
             mc:Ignorable="d"  
             d:DataContext="{x:Static Designer:DesignerContext.Editor}"  
             d:DesignWidth="600" d:DesignHeight="600"  
             x:DataType="Editor:ProjectEditorViewModel" x:CompileBindings="False">

    <!--  Width="{Binding $self.Container.Width}" Height="{Binding $self.Container.Height}" -->
</UserControl>
